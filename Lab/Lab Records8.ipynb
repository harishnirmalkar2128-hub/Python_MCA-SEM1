   "source": [
    "# Input list of students and marks\n",
    "students = [('Nidhi', 89), ('Harish', 72), ('Sameer', 79), ('Chaitanya', 82)]\n",
    "\n",
    "# Find the student with the highest marks\n",
    "topper = max(students, key=lambda x: x[1])\n",
    "\n",
    "# Display the result\n",
    "print(f\"Topper: {topper[0]}({topper[1]})\")\n"
   ]
  }
