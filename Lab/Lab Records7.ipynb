    "# Input list\n",
    "items = [\"apple\", \"banana\", \"apple\", \"orange\", \"banana\"]\n",
    "\n",
    "# Remove duplicates using set and sort the list\n",
    "unique_sorted_items = sorted(list(set(items)))\n",
    "\n",
    "# Display the result\n",
    "print(\"Unique & Sorted Items:\", unique_sorted_items)\n"
   ]
