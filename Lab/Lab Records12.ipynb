    "import random\n",
    "\n",
    "# List of students\n",
    "students = [\"Nidhi\", \"Harish\", \"Sameer\", \"Chaitanya\"]\n",
    "\n",
    "# Select CR randomly\n",
    "cr = random.choice(students)\n",
    "print(\"Class Representative (CR):\", cr)\n",
    "\n",
    "# Shuffle the list\n",
    "random.shuffle(students)\n",
    "print(\"Shuffled list of students:\", students)\n"
   ]
  }
