   "source": [
    "def factorial(n):\n",
    "    if n == 0 or n == 1: # Base case\n",
    "        return 1\n",
    "    return n * factorial(n - 1)  # Recursive call\n",
    "\n",
    "# Example Usage\n",
    "num = int(input(\"Enter a number: \"))\n",
    "\n",
    "print(\"Factorial of\", num, \"is\", factorial(num))"
   ]
  }
