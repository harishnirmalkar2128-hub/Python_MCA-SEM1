   "source": [
    "from shap import point, circle\n",
    "\n",
    "# Create Point object\n",
    "center = point(2, 3)\n",
    "\n",
    "# Create Circle object\n",
    "c = circle(center, 5)\n",
    "\n",
    "print(c.display())\n",
    "print(\"Area:\", c.area())\n",
    "print(\"Circumference:\", c.circumference())\n"
   ]
  }
